{
  "name": "such-cli",
  "description": "A command line tool for generating fake data and initialize the suchjs config, customizable and powerful.",
  "version": "0.2.1",
  "author": "jxz_211@163.com",
  "bin": {
    "such": "bin/such.js"
  },
  "engines": {
    "node": "^12.20.0 || ^14.13.1 || >=16.0.0"
  },
  "keywords": [
    "suchjs",
    "such-cli",
    "fake",
    "mock",
    "data",
    "cli",
    "command line"
  ],
  "scripts": {
    "lint": "npx eslint --ext .js lib/",
    "prepublish": "npm run lint",
    "pkg": "pkg . && tar -zcvf dist/such-$npm_package_version.tar.gz dist/such-cli"
  },
  "dependencies": {
    "chalk": "^4.1.2",
    "cli-spinners": "^2.6.1",
    "commander": "^8.3.0",
    "inquirer": "^8.2.0",
    "suchjs": "^2.2.0"
  },
  "devDependencies": {
    "eslint": "^8.7.0"
  },
  "pkg": {
    "targets": [
      "node16-macos-x64"
    ],
    "outputPath": "dist",
    "version": "$npm_package_version"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/suchjs/such-cli"
  },
  "bugs": {
    "url": "https://github.com/suchjs/such-cli/issues"
  },
  "license": "MIT"
}
